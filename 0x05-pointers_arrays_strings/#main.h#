#include "main.h"

/**
 * _strlen - Returns the length of a string.
 * @s: pointer to the string
 *
 * Return: the length of the string
 */
int _strlen(char *s)
{
	int length = 0;

	while (*s != '\0')
	{
		length++;
		s++;
	}

	return (length);
}
void swap_int(int *a, int *b);
void reset_to_98(int *n);
